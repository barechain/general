    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "laravel/framework": ">=9.0"
    },
    "require-dev": {
        "psy/psysh": "^0.11.2"
    },
    "autoload": {
        "psr-4": {
            "Barechain\\General\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
