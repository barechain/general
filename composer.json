{
    "name": "barechain/general",
    "type": "library",
    "description": "General Laravel abstract classes",
    "keywords": ["barechain", "general"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Khatsko",
            "email": "y.khatsko@codiv.net"
        }
