{
    "name": "barechain/general",
    "type": "library",
    "description": "General Laravel abstract classes",
    "keywords": ["barechain", "general"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Khatsko",
            "email": "y.khatsko@codiv.net"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "laravel/framework": ">=9.0",
        "sentry/sentry-laravel": "^2.14.2"
    },
    "autoload": {
        "psr-4": {
            "Barechain\\General\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barechain\\General\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "require-dev": {
        "psy/psysh": "^0.11.2"
    }
}
